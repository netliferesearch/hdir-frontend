{
  "name": "helse",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "animate-scss": "^3.2.1",
    "classnames": "^2.2.6",
    "flexboxgrid-sass": "^8.0.5",
    "node-sass-chokidar": "^1.3.3",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-autosuggest": "^9.4.1",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "sass-mq": "^5.0.0",
    "shortid": "^2.2.13",
    "zenscroll": "^4.0.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js && cp build/index.html build/404.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "start:lambda": "netlify-lambda serve src/faas"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^4.0.12",
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-knobs": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addon-notes": "^4.0.12",
    "@storybook/addon-viewport": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "loki": "^0.14.0",
    "netlify-lambda": "^1.1.0",
    "react-storybook-addon-static-markup": "^0.1.0",
    "storybook-addon-smart-knobs": "^3.3.1"
  },
  "loki": {
    "configurations": {
      "chrome.desktop": {
        "target": "chrome.app",
        "width": 1920,
        "height": 1080,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false,
        "diffingEngine": "gm"
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
